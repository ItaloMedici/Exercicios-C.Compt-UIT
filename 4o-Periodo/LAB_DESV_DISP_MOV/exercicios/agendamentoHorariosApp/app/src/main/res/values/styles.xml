<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ContainerInputsLogin" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">344dp</item>
        <item name="android:background">@drawable/container_inputs_shape</item>
    </style>
    
    <style name="EditTextInput" >
        <item name="android:background">@drawable/edit_text</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:maxLength">25</item>
        <item name="android:layout_marginLeft">29dp</item>
        <item name="android:layout_marginRight">29dp</item>
    </style>

    <style name="ButtonEntrar" >
        <item name="android:background">@drawable/button</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginLeft">29dp</item>
        <item name="android:layout_marginRight">29dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/robotoMedium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_marginTop">15dp</item>

    </style>
</resources>